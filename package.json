{
  "name": "git-cheatsheet",
  "version": "0.1.1",
  "description": "A filterable cheatsheet for the Git control version system",
  "scripts": {
    "start": "node tools/src-server.js",
    "dist": "node tools/build.js",
    "dist:serve": "npm run dist && node tools/dist-server.js",
    "deploy": "cd dist && git push -f git@github.com:soyguijarro/git-cheatsheet.git master:gh-pages && cd ..",
    "lint": "eslint src",
    "prestart": "npm prune && npm install",
    "predist": "npm prune && npm install && [ ! -d dist ] || rm -r -f dist",
    "predeploy": "npm run dist && cd dist && git init && git add . && git commit -m 'Deploy to GitHub Pages'",
    "preversion": "[[ $(git rev-parse --abbrev-ref HEAD) == *master* ]] || { echo 'Not on master branch'; exit 1; }",
    "postversion": "git push --follow-tags origin master && npm run deploy"
  },
  "devDependencies": {
    "autoprefixer": "6.3.6",
    "babel": "6.5.2",
    "babel-core": "6.7.6",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.6.0",
    "browser-sync": "2.11.2",
    "colors": "1.1.2",
    "css-loader": "0.23.1",
    "escape-string-regexp": "1.0.5",
    "eslint": "2.7.0",
    "eslint-config-airbnb": "6.2.0",
    "eslint-loader": "1.3.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.15.0",
    "image-webpack-loader": "1.6.3",
    "json-loader": "0.5.4",
    "node-sass": "3.4.2",
    "normalize.css": "4.0.0",
    "postcss-loader": "0.8.2",
    "sass-loader": "3.2.0",
    "style-loader": "0.13.1",
    "stylelint": "5.4.0",
    "stylelint-config-standard": "5.0.0",
    "stylelint-webpack-plugin": "0.1.0",
    "url-loader": "0.5.7",
    "webpack": "1.12.14",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.10.0",
    "webpack-notifier": "1.3.0"
  },
