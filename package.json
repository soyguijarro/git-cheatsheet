{
  "name": "git-cheatsheet",
  "version": "0.2.1",
  "description": "A filterable cheatsheet for the Git version control system",
  "scripts": {
    "start": "node tools/src-server.js",
    "build": "node tools/build.js",
    "build:serve": "npm run build && node tools/dist-server.js",
    "deploy": "cd dist && git push -f git@github.com:soyguijarro/git-cheatsheet.git master:gh-pages && cd ..",
    "lint": "eslint src && stylelint src/**/*.scss",
    "prestart": "if [ ! -d node_modules ] ; then npm install; fi",
    "prebuild": "npm install && rm -r -f dist",
    "predeploy": "npm run build && cd dist && echo 'gitcheatsheet.soyguijarro.com' > CNAME && git init && git add . && git commit -m 'Deploy'",
    "preversion": "[[ $(git rev-parse --abbrev-ref HEAD) == *master* ]] || { echo 'Not on master branch'; exit 1; }",
    "postversion": "git push --follow-tags origin master && npm run deploy"
  },
  "dependencies": {
    "escape-string-regexp": "1.0.5"
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel": "6.5.2",
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-loader": "6.2.5",
    "babel-preset-es2015": "6.14.0",
    "babel-preset-stage-2": "6.13.0",
    "browser-sync": "2.16.0",
    "colors": "1.1.2",
    "css-loader": "0.25.0",
    "eslint": "3.6.0",
    "eslint-config-airbnb-base": "7.2.0",
    "eslint-loader": "1.5.0",
    "eslint-plugin-import": "1.16.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "json-loader": "0.5.4",
    "node-sass": "3.10.0",
    "normalize.css": "4.2.0",
    "postcss-loader": "0.13.0",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "stylelint": "7.3.1",
    "stylelint-config-standard": "13.0.2",
    "stylelint-webpack-plugin": "0.3.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.2",
    "webpack-hot-middleware": "2.12.2",
    "webpack-notifier": "1.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/soyguijarro/git-cheatsheet.git"
  },
  "author": {
    "name": "Ram√≥n Guijarro",
    "email": "hola@soyguijarro.com",
    "url": "http://soyguijarro.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/soyguijarro/git-cheatsheet/issues"
  },
  "homepage": "https://github.com/soyguijarro/git-cheatsheet#readme"
}
